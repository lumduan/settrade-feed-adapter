# Settrade Open API Credentials
# Copy this file to .env and fill in your values
# Get credentials from: https://open-api.settrade.com

SETTRADE_APP_ID=your_app_id_here
# Note: APP_SECRET is base64-encoded. Padding ('=') is added automatically if needed.
SETTRADE_APP_SECRET=your_app_secret_here
SETTRADE_BROKER_ID=your_broker_id_here
SETTRADE_APP_CODE=your_app_code_here
SETTRADE_EQUITY_ACCOUNT=your_equity_account_here
SETTRADE_DERIVATIVES_ACCOUNT=your_derivatives_account_here
SETTRADE_PIN=your_pin_here
